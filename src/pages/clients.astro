---
import Container from "@components/container.astro";
import { Picture } from "astro:assets";
import abm from "../assets/abm.png";
import boring from "../assets/boring.png";
import doceo from "../assets/doceo.png";
import gallivan from "../assets/gallivan.png";
import kelley from "../assets/kelley.png";
import knight from "../assets/knight.png";
import laserfax from "../assets/laserfax.png";
import mca from "../assets/mca.png";
import msi from "../assets/msi.png";
import pbs from "../assets/pbs.png";
import standley from "../assets/standley.png";
import ubs from "../assets/ubs.png";
import Layout from "@layouts/Layout.astro";
