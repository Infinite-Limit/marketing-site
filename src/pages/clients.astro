        <div class="grid grid-cols-2 md:grid-cols-4 gap-20 mx-auto max-w-6xl mt-12 items-center">
            <Picture
            src={laserfax}
            alt={"laserfax"}
            sizes="(max-width: 800px) 100vw, 400px"
